cond %{READ_REQUEST_HDR_HOOK}

# Remove for HTTPoxy.
rm-header Proxy

# Show cache status in response.
rm-header X-Debug
set-header X-Debug x-cache

# Remove Google Analytics cookies; the backend doesn't need to see these.
rm-cookie _ga
rm-cookie _gat
rm-cookie __utma
rm-cookie __utmb
rm-cookie __utmt
rm-cookie __utmc
rm-cookie __utmz
rm-cookie __utmv
rm-cookie __utmx
rm-cookie __utmxx
rm-cookie __gaexp
