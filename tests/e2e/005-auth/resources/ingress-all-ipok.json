{
    "apiVersion": "extensions/v1beta1",
    "kind": "Ingress",
    "metadata": {
        "name": "echoheaders-all-ipok",
        "namespace": "default",
        "annotations": {
            "ingress.kubernetes.io/auth-secret": "authtest",
            "ingress.kubernetes.io/auth-realm": "auth test",
            "ingress.kubernetes.io/auth-type": "basic",
            "ingress.kubernetes.io/whitelist-source-range": "127.0.0.1/32",
            "ingress.kubernetes.io/auth-satisfy": "all"
        }
    },
    "spec": {
        "rules": [
            {
                "host": "all-ipok.echoheaders.test",
                "http": {
                    "paths": [
                        {
                            "backend": {
                                "serviceName": "echoheaders",
                                "servicePort": "http"
                            }
                        }
                    ]
                }
            }
        ]
    }
}
