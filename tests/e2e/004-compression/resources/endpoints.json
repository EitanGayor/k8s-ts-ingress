{
    "apiVersion": "v1",
    "kind": "Endpoints",
    "metadata": {
        "name": "echoheaders",
        "namespace": "default"
    },
    "subsets": [
        {
            "addresses": [
                {
                    "ip": "$TEST_IP_ADDRESS"
                }
            ],
            "ports": [
                {
                    "name": "http",
                    "port": 48080,
                    "protocol": "TCP"
                }
            ]
        }
    ]
}
