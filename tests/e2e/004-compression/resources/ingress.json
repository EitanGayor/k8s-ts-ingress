    "kind": "Ingress",
    "metadata": {
        "name": "echoheaders",
        "namespace": "default"
    },
    "spec": {
        "rules": [
            {
                "host": "echoheaders.test",
                "http": {
                    "paths": [
                        {
                            "backend": {
                                "serviceName": "echoheaders",
                                "servicePort": "http"
                            }
                        }
                    ]
                }
            }
        ]
    }
}
